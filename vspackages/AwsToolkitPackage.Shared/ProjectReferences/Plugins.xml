    <Content Include="$(BuildRoot)\thirdparty\PemToPPKConverter.exe">
      <Link>Plugins\PemToPPKConverter.exe</Link>
      <IncludeInVSIX>true</IncludeInVSIX>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
    <Content Include="$(BuildRoot)\thirdparty\dotnet-lambda.dll">
      <Link>Plugins\dotnet-lambda.dll</Link>
      <IncludeInVSIX>true</IncludeInVSIX>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
    <Content Include="$(BuildRoot)\awsdeploy\client\AWSDeploymentLib\bin\$(Configuration)\AWSDeploymentLib.dll">
      <Link>Plugins\AWSDeploymentLib.dll</Link>
      <IncludeInVSIX>true</IncludeInVSIX>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
    <Content Include="$(BuildRoot)\toolkitcore\AmazonCLIExtensions\Amazon.Lambda.Tools\Resources\build-lambda-zip.exe">
      <Link>Resources\build-lambda-zip.exe</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <IncludeInVSIX>true</IncludeInVSIX>
    </Content>
    <Content Include="$(OutputRoot)\AmazonCLIExtensions.dll">
      <Link>AmazonCLIExtensions.dll</Link>
