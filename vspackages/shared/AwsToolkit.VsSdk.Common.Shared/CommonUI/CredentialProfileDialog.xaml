<local:ThemedDialogWindow
    x:Class="AwsToolkit.VsSdk.Common.CommonUI.CredentialProfileDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:behaviors="clr-namespace:AwsToolkit.VsSdk.Common.CommonUI.Behaviors"
    xmlns:local="clr-namespace:AwsToolkit.VsSdk.Common.CommonUI"
    xmlns:themes="clr-namespace:Amazon.AWSToolkit.Themes;assembly=AWSToolkit"
    xmlns:profiles="clr-namespace:Amazon.AWSToolkit.CommonUI.CredentialProfiles;assembly=AWSToolkit"
    behaviors:WindowBehavior.BindDialogResult="{Binding DialogResult}"
    themes:ToolkitThemes.UseToolkitTheme="True"
    mc:Ignorable="d"
    d:DesignHeight="625"
    d:DesignWidth="525"
    Height="625"
    MaxHeight="725"
    Width="525"
    ResizeMode="NoResize">
    <profiles:CredentialProfileForm
        DataContext="{Binding CredentialProfileFormViewModel}" />
</local:ThemedDialogWindow>
