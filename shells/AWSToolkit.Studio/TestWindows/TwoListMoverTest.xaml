<Window x:Class="Amazon.AWSToolkit.Studio.TestWindows.TwoListMoverTest"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:primitives="clr-namespace:Amazon.AWSToolkit.Primitives;assembly=AWSToolkit"
    DataContext="{Binding RelativeSource={RelativeSource Self}}"    
    x:Name="Main"        
    Title="TwoListMoverTest" Height="300" Width="300">
    <Grid>
        <primitives:TwoListMover Name="_ctlTwoListMover" AvailableItems="{Binding ElementName=Main,Path=Available}" AssignedItems="{Binding ElementName=Main,Path=Assigned}" />
    </Grid>
</Window>
