    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="AWSStudioWindow.xaml" 
    >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/_AWSStudioThemeOverrides.xaml" />
