{
    "types": [
        {
            "name": "lambdaArchitecture",
            "allowedValues": [ "x86_64", "arm64" ],
            "description": "The Lambda Architecture of the function"
        }
    ],
    "metrics": [
        {
            "name": "lambda_deploy",
            "description": "Called when deploying a Lambda Function",
            "metadata": [
                { "type": "lambdaPackageType" },
                { "type": "result" },
                { "type": "initialDeploy" },
                {
                    "type": "runtime",
                    "required": false
                },
                {
                    "type": "platform",
                    "required": false
                },
                {
                    "type": "lambdaArchitecture",
                    "required": false
                }
            ]
        }
    ]
}

