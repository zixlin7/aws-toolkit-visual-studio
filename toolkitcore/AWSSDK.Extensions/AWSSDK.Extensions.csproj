<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyTitle>AWS Toolkit for Visual Studio - AWS SDK Extensions</AssemblyTitle>
    <RootNamespace>AWSSDK.Extensions</RootNamespace>
    <UseCommonOutputDirectory>true</UseCommonOutputDirectory>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.AutoScaling" Version="3.7.302.10" />
    <PackageReference Include="AWSSDK.CloudFormation" Version="3.7.303.8" />
    <PackageReference Include="AWSSDK.ElasticLoadBalancing" Version="3.7.300.52" />
    <PackageReference Include="AWSSDK.IdentityManagement" Version="3.7.300.52" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.305.28" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.301.0" />
    <PackageReference Include="AWSSDK.SQS" Version="3.7.300.52" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>
</Project>