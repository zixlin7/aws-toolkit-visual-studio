<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyTitle>AWS Toolkit for Visual Studio - AWS SDK Extensions</AssemblyTitle>
    <RootNamespace>AWSSDK.Extensions</RootNamespace>
    <UseCommonOutputDirectory>true</UseCommonOutputDirectory>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.AutoScaling" Version="3.7.103.14" />
    <PackageReference Include="AWSSDK.CloudFormation" Version="3.7.104.24" />
    <PackageReference Include="AWSSDK.ElasticLoadBalancing" Version="3.7.100.86" />
    <PackageReference Include="AWSSDK.IdentityManagement" Version="3.7.100.87" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.103.23" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.101.22" />
    <PackageReference Include="AWSSDK.SQS" Version="3.7.100.86" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>
</Project>
