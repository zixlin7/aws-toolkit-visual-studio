<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <PropertyGroup>
    <AssemblyTitle>AWSToolkit.S3</AssemblyTitle>
    <RootNamespace>Amazon.AWSToolkit.S3</RootNamespace>
    <UseCommonOutputDirectory>true</UseCommonOutputDirectory>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="AWSSDK.CloudFront" Version="3.7.105.8" />
    <PackageReference Include="AWSSDK.IdentityManagement" Version="3.7.100.87" />
    <PackageReference Include="AWSSDK.Lambda" Version="3.7.105.6" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.103.23" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.7.101.22" />
    <PackageReference Include="AWSSDK.SQS" Version="3.7.100.86" />
    <PackageReference Include="log4net" Version="2.0.15" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="$(BuildRoot)\toolkitcore\AWSSDK.Extensions\AWSSDK.Extensions.csproj" />
    <ProjectReference Include="$(BuildRoot)\toolkitcore\AWSToolkit.Util\AWSToolkit.Util.csproj" />
    <ProjectReference Include="$(BuildRoot)\toolkitcore\AWSToolkit\AWSToolkit.csproj" />
    <ProjectReference Include="$(BuildRoot)\toolkitcore\plugins\AWSToolkit.CloudFront.Interface\AWSToolkit.CloudFront.Interface.csproj" />
    <ProjectReference Include="$(BuildRoot)\toolkitcore\plugins\AWSToolkit.PolicyEditor\AWSToolkit.PolicyEditor.csproj" />
    <ProjectReference Include="$(BuildRoot)\toolkitcore\plugins\AWSToolkit.S3.Interface\AWSToolkit.S3.Interface.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource 
      Include="Resources\EmbeddedImages\*.png"
      Exclude="Resources\EmbeddedImages\cloudfront-distribution.png" />
    <Resource Include="Resources\EmbeddedImages\cloudfront-distribution.png" />
    <Resource Include="Resources\Images\*.png" />
  </ItemGroup>
</Project>
