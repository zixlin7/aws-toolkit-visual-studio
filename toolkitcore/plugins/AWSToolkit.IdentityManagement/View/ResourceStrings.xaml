<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="ToolGroupPath">Optionally give a group a path that you define. You might use the path to identify which division or part of the organization the group belongs in. For example: /division_abc/subdivision_xyz/product_1234/engineering/.</system:String>
    <system:String x:Key="ToolUserPath">Optionally give a user a path that you define. You might use the path to identify which division or part of the organization the user belongs in. For example: /division_abc/subdivision_xyz/product_1234/engineering/.</system:String>
    
</ResourceDictionary>
