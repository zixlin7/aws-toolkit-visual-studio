<!-- AWS Toolkit theme defaults defining colors and brushes for use across all shells.
-->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Geometry x:Key="awsDownArrowGeometry">M 0 0 L 3.5 4 L 7 0 Z</Geometry>
    <Geometry x:Key="awsUpArrowGeometry">M 0,4 L 3.5,0 L 7,4 Z</Geometry>
    <Geometry x:Key="awsRightArrowGeometry">M 0,0 L 4,3.5 L 0,7 Z</Geometry>
    <Geometry x:Key="awsLeftArrowGeometry">M 0,3.5 L 4,7 L 4,0 Z</Geometry>

    <Geometry x:Key="awsCheckmarkGeometry">M 0,5.1 L 1.7,5.2 L 3.4,7.1 L 8,0.4 L 9.2,0 L 3.3,10.8 Z</Geometry>

</ResourceDictionary>